{
  "name": "dahlia-backend",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix && prettier --write .",
    "format": "prettier --write .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "db:create": "npx sequelize db:create --config ./src/config/sequelize-config.js",
    "migrate": "npx sequelize-cli db:migrate --config ./src/config/sequelize-config.js --models-path ./src/models --migrations-path ./src/migrations",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config ./src/config/sequelize-config.js --models-path ./src/models --migrations-path ./src/migrations"
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^12.3.0",
    "google-auth-library": "^9.9.0",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "nodejs": "^0.0.0",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "7.0.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  }
}
